{
  "manifest_version": 3,
  "name": "Reading time",
  "version": "1.0",
  "description": "Add the reading time to Chrome Extension documentation articles",
  "content_scripts": [
    {
      "js": ["assets/index-1185d0ff.js"],
      "matches": [
        "https://*/*",
        "http://*/*"
      ]
    }
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabGroups"
  ]
}